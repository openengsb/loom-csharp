<Window x:Class="Org.OpenEngSB.DotNet.Lib.MockupMonitor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="1" Name="tbStatus" Background="Gray">Status</TextBlock>
        <ListBox Name="lstMethods" SelectionMode="Single" MouseDoubleClick="lstMethods_MouseDoubleClick" />
    </Grid>
</Window>
